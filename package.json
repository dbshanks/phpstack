    "cross-env": "^7.0.2",
    "gsap": "^3.5.1",
    "laravel-mix": "^5.0.9",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.29.0",
    "sass-loader": "^10.0.5",
    "vue-template-compiler": "^2.6.12"
